{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3000 next dev --turbopack",
    "dev:clean": "./dev-server.sh",
    "dev:force": "pkill -f 'next dev' || true && lsof -t -i:3000 | xargs kill -9 2>/dev/null || true && sleep 2 && PORT=3000 npm run dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-storybook": "^9.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "@storybook/nextjs-vite": "^9.1.5",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
